{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": ".",
        "rootDir": ".",
        "outDir": "dist",
        "removeComments": true,
        "sourceMap": true,
        "declaration": true,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "inlineSources": true,
        "target": "es5",
        "skipLibCheck": true,
        "stripInternal": true,
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es2017",
            "dom"
        ]
    },
    "files": [
        "geco-dialog.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ],
    "angularCompilerOptions": {
        "strictMetadataEmit": true
    },
}